<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkWindow" id="window1">
    <property name="can_focus">False</property>
    <child>
      <object class="GtkLabel" id="label">
        <property name="width_request">300</property>
        <property name="height_request">300</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="tooltip_markup" translatable="yes">Drop &lt;b&gt;video&lt;/b&gt; files to be processed here.
They will automatically be converted to &lt;b&gt;M4V,
H264/AAC&lt;/b&gt; and their audio normalized.</property>
        <property name="label" translatable="yes">&lt;b&gt;Drop something on me !&lt;/b&gt;</property>
        <property name="use_markup">True</property>
        <property name="wrap">True</property>
      </object>
    </child>
  </object>
</interface>
